{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "title": {
        "text": "DOGE Price",
        "subtitle": "Price at close (GBP), Source: CoinAPI.io",
        "subtitleFontStyle": "italic",
        "subtitleFontSize": 10,
        "anchor": "start",
        "color": "black"
    },
    "data": {
        "url": "https://raw.githubusercontent.com/andrewsnowdon/andrewsnowdon.github.io/main/data_COINAPI-DOGE.json"
    },
    "height": 300,
    "width": 300,
    "mark": {
        "type": "line",
        "point": false,
        "clip": true,
        "color": "green"
    },
    "encoding": {
        "x": {
            "field": "time_period_start",
            "type": "temporal",
            "title": null,
            "axis": {
                "grid": false
            }
        },
        "y": {
            "field": "price_close",
            "type": "quantitative",
            "title": null,
            "axis": {
                "grid": false
            }
        }
    }
}
